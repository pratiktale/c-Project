#include <iostream>
using namespace std;

int main() {
    int n, rev = 0;
    cout << "Enter a positive integer: ";
    cin >> n;
    while (n != 0) {
        int digit = n % 10;
        rev = rev * 10 + digit;
        n /= 10;
    }
    cout << "The reversed number is: " << rev << endl;
    return 0;
}
